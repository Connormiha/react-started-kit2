{
  "name": "react-started-kit",
  "description": "Empty project for Single Page Application. Stack technologies: TypeScript, Webpack, React, Redux, Stylus, PostCSS, Jest",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov <connormiha1@gmail.com>",
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/react-started-kit2.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "gulp lint",
    "postinstall": "typings install",
    "start": "webpack-dev-server",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "styl",
      "css"
    ],
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testRegex": "/__tests__/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "notify": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.ts"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "bem-cn": "2.1.2",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "gulp": "^3.9.1",
    "gulp-stylint": "^3.0.0",
    "gulp-tslint": "^6.1.1",
    "html-webpack-plugin": "2.22.0",
    "jest-cli": "^15.1.1",
    "json-loader": "0.5.4",
    "lodash": "^4.16.1",
    "postcss": "5.2.0",
    "poststylus": "^0.2.3",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "seamless-immutable": "6.1.1",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "webpack-sources": "0.1.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "NODE_ENV=pre_commit npm run lint",
      "pre-push": "npm test"
    }
  }
}
